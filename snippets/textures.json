  },
  "THREE.CanvasTexture": {
    "prefix": "canvastexture",
    "body": "new THREE.CanvasTexture(${1:canvas}, ${2:mapping}, ${3:wrapS}, ${4:wrapT}, ${5:magFilter}, ${6:minFilter}, ${7:format}, ${8:type}, ${9:anisotropy})${0}"
  },
